{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    {
      "identifier": "fs:read-dirs",
      "allow": ["**"]
    },
    {
      "identifier": "fs:allow-copy-file",
      "allow": ["**"]
    },
    {
      "identifier": "fs:allow-mkdir",
      "allow": ["**"]
    },
    {
      "identifier": "fs:allow-remove",
      "allow": ["**"]
    },
    "dialog:default"
  ]
}